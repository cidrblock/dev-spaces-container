{
	"name": "ansible-dev-container-podman",
	"build": {
		"dockerfile": "Containerfile"
	},
	"containerUser": "user",
	"runArgs": [
		"--userns=keep-id:uid=1000,gid=1000",
		"--hostname=ansible-dev-container"
	],
	"updateRemoteUserUID": true,
	"customizations": {
		"vscode": {
			"extensions": [
				"redhat.ansible"
			]
		}
	}
}